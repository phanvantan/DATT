<p id="notice"><%= notice %></p>

<h1>User Tours</h1>

<table>
  <thead>
    <tr>
      <th>User</th>
      <th>Tour</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @user_tours.each do |user_tour| %>
      <tr>
        <td><%= user_tour.user_id %></td>
        <td><%= user_tour.tour_id %></td>
        <td><%= link_to 'Show', user_tour %></td>
        <td><%= link_to 'Edit', edit_user_tour_path(user_tour) %></td>
        <td><%= link_to 'Destroy', user_tour, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New User Tour', new_user_tour_path %>
